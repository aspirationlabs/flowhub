{
  "name": "flowhub",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "format": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "dev": "next dev",
    "build": "pnpm run generate-icons && pnpm run build:web && pnpm run build:extension",
    "build:web": "next build --webpack",
    "build:extension": "FLOWHUB_BUILD_TARGET=extension next build --webpack && FLOWHUB_BUILD_TARGET=extension pnpm run export-extension",
    "generate-icons": "node scripts/generate-icons.mjs",
    "export-extension": "node scripts/export-extension.mjs",
    "fetch-backgrounds": "node scripts/fetch-backgrounds.mjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/chrome": "^0.1.27",
    "@types/jest": "29.5.12",
    "@types/node": "24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "autoprefixer": "^10.4.21",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.18",
    "ts-jest": "29.4.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-switch": "^1.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  }
}
