{
  "name": "flowhub",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "format": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "build": "pnpm run generate-icons && webpack --mode production",
    "watch": "webpack --watch",
    "generate-icons": "ts-node typescript/scripts/generate-icons.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/chrome": "^0.1.27",
    "@types/jest": "29.5.12",
    "@types/node": "24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "html-webpack-plugin": "^5.6.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "3.6.2",
    "sharp": "^0.34.4",
    "style-loader": "^4.0.0",
    "ts-jest": "29.4.3",
    "ts-loader": "^9.5.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "tailwindcss": "^4.1.11",
    "postcss": "^9.0.1",
    "autoprefixer": "^11.0.4"
  },
  "dependencies": {
    "next": "^15.1.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^3.24.1"
  }
}
