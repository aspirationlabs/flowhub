{
  "name": "flowhub",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @flowhub/typescript run dev",
    "build": "pnpm --filter @flowhub/typescript run build",
    "build:web": "pnpm --filter @flowhub/typescript run build:web",
    "build:extension": "pnpm --filter @flowhub/typescript run build:extension",
    "start": "pnpm --filter @flowhub/typescript run start",
    "lint": "pnpm --filter @flowhub/typescript run lint",
    "typecheck": "pnpm --filter @flowhub/typescript run typecheck",
    "format": "pnpm --filter @flowhub/typescript run format",
    "test": "pnpm --filter @flowhub/typescript run test",
    "test:watch": "pnpm --filter @flowhub/typescript run test:watch",
    "test:e2e": "pnpm --filter @flowhub/typescript run test:e2e",
    "export-extension": "pnpm --filter @flowhub/typescript run export-extension",
    "generate-icons": "pnpm --filter @flowhub/typescript run generate-icons",
    "fetch-backgrounds": "pnpm --filter @flowhub/typescript run fetch-backgrounds"
  }
}